set terminal 'gif' size 1600, 800
set output 'output.gif'
set multiplot layout 2,1 columnsfirst scale 1.0,1.0
set rmargin 32
set lmargin 8
set tmargin 0
set bmargin 0
set datafile separator ','
set style data lines
set grid
set key autotitle columnhead
set key outside font ",10"
plot './tmp/allrestops.csv' using 1:5 title columnheader
plot './tmp/allrestops.csv' using 1:6 title columnheader
pause -1 "Press any key to continue"
