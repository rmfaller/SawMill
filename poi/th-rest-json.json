{
  "timestampformat": "yyyy-MM-dd'T'HH:mm:ss",
  "fielddelimiter": "JSON",
  "timestampfield": "timestamp",
  "timescale": "elapsedTimeUnits",
  "base-poi": [
    "DELETE-s",
    "DELETE-f",
    "GET-query-s",
    "GET-query-f",
    "GET-s",
    "GET-d",
    "GET-f",
    "PATCH-s",
    "PATCH-f",
    "POST-authenticate-s",
    "POST-authenticate-f",
    "POST-action-s",
    "POST-action-f",
    "POST-s",
    "POST-f",
    "PUT-s",
    "PUT-f"
  ],
  "poi": [
    "DELETE",
    "GET",
    "PATCH",
    "POST",
    "PUT"
  ],
    "DELETE": {
        "identifiers": ["\"method\":\"DELETE\""],
        "lapsedtimefield": "elapsedTime",
        "sla": 100
    },
    "GET-query-s": {
        "identifiers": ["\"method\":\"GET\"", "\"status\":\"SUCCESSFUL\"", "_queryFilter="],
        "lapsedtimefield": "elapsedTime",
        "sla": 400
    },
    "GET-query-f": {
        "identifiers": ["\"method\":\"GET\"", "\"status\":\"FAILED\"", "_queryFilter="],
        "lapsedtimefield": "elapsedTime",
        "sla": 500
    },
    "GET": {
        "identifiers": ["\"method\":\"GET\""],
        "lapsedtimefield": "elapsedTime",
        "sla": 100
    },
    "GET-s": {
        "identifiers": ["\"method\":\"GET\"", "\"status\":\"SUCCESSFUL\""],
        "lapsedtimefield": "elapsedTime",
        "sla": 400
    },
    "GET-d": {
        "identifiers": ["\"method\":\"GET\"", "\"status\":\"DENIED\""],
        "lapsedtimefield": "elapsedTime",
        "sla": 500
    },
    "GET-f": {
        "identifiers": ["\"method\":\"GET\"", "\"status\":\"FAILED\""],
        "lapsedtimefield": "elapsedTime",
        "sla": 500
    },
    "PATCH": {
        "identifiers": ["\"method\":\"PATCH\""],
        "lapsedtimefield": "elapsedTime",
        "sla": 100
    },
    "PATCH-s": {
        "identifiers": ["\"method\":\"PATCH\"", "\"status\":\"SUCCESSFUL\""],
        "lapsedtimefield": "elapsedTime",
        "sla": 100
    },
    "PATCH-f": {
        "identifiers": ["\"method\":\"PATCH\"", "\"status\":\"FAILED\""],
        "lapsedtimefield": "elapsedTime",
        "sla": 100
    },
    "POST-authenticate-s": {
        "identifiers": ["\"method\":\"POST\"", "\"status\":\"SUCCESSFUL\"", "/json/realms/root/authenticate\""],
        "lapsedtimefield": "elapsedTime",
        "sla": 100
    },
    "POST-authenticate-f": {
        "identifiers": ["\"method\":\"POST\"", "\"status\":\"FAILED\"", "/json/realms/root/authenticate\""],
        "lapsedtimefield": "elapsedTime",
        "sla": 100
    },
    "POST-action-s": {
        "identifiers": ["\"method\":\"POST\"", "\"status\":\"SUCCESSFUL\"", "_action="],
        "lapsedtimefield": "elapsedTime",
        "sla": 100
    },
    "POST-action-f": {
        "identifiers": ["\"method\":\"POST\"", "\"status\":\"FAILED\"", "_action="],
        "lapsedtimefield": "elapsedTime",
        "sla": 100
    },
    "POST": {
        "identifiers": ["\"method\":\"POST\""],
        "lapsedtimefield": "elapsedTime",
        "sla": 100
    },
    "POST-s": {
        "identifiers": ["\"method\":\"POST\"", "\"status\":\"SUCCESSFUL\""],
        "lapsedtimefield": "elapsedTime",
        "sla": 100
    },
    "POST-f": {
        "identifiers": ["\"method\":\"POST\"", "\"status\":\"FAILED\""],
        "lapsedtimefield": "elapsedTime",
        "sla": 100
    },
    "PUT": {
        "identifiers": ["\"method\":\"PUT\""],
        "lapsedtimefield": "elapsedTime",
        "sla": 100
    },
    "PUT-s": {
        "identifiers": ["\"method\":\"PUT\"", "\"status\":\"SUCCESSFUL\""],
